                "${fileDirname}/MyCmdDos/CsvSplit.c",
                "${fileDirname}/C_MyLib/JsonDataDoneLib.c",
                "${fileDirname}/C_MyLib/NumberBaseLib.c",
                "${fileDirname}/C_MyLib/JsonDataAnalyzeLib.c",
                "${fileDirname}/C_MyLib/StrLib.c",
                "${fileDirname}/C_MyLib/WinExcel.c",
                "${fileDirname}/C_MyLib/JsonCheckFun.c",
                "${fileDirname}/C_MyLib/SendDataBySerial.c",
                "-o",
